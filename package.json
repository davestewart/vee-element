{
  "name": "vee-element",
  "version": "1.0.0-alpha.3",
  "description": "Replaces Element UI's validation engine with Vee Validate",
  "author": "Dave Stewart",
  "homepage": "https://davestewart.github.io/vee-element/",
  "repository": {
    "type": "git",
    "url": "https://github.com/davestewart/vee-element"
  },
  "keywords": [
    "element-ui",
    "vee-validate",
    "validation",
    "forms",
    "vue"
  ],
  "main": "dist/vee-element.js",
  "module": "dist/vee-element.esm.js",
  "jsnext:main": "dist/vee-element.esm.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "rollup -c build/rollup.js -w",
    "build": "rollup -c build/rollup.js",
    "demo": "cd demo && npm run dev",
    "lint": "./node_modules/.bin/eslint src/*",
    "test": "jest --watchAll --verbose",
    "unit": "jest --config jest.config.js --coverage"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davestewart/vee-element/issues"
  },
  "dependencies": {
    "object-get": "^2.1.0"
  },
  "devDependencies": {
    "babel-jest": "^23.4.0",
    "eslint": "^5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.4.1",
    "rollup-plugin-buble": "^0.19.1",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-license": "^0.6.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
